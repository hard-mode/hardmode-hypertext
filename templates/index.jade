doctype html

html

  head
    title= title
    link(rel='stylesheet', href='/style')
    meta(charset='utf-8')

  body
    script(src='/script')

    script(type='application/wisp')
      |
      |!
        (let [client (require "client")]
          (client.init-widgets!
      - for (var i in body)
        - var member = body[i]
        |
        |!     { :id       "!{mori.get(member, 'id')}"
        |!       :template "!{mori.get(member, 'template')}" }
      | ))
      |

